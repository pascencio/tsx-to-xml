{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "jsx": "react-jsx",
    "jsxImportSource": "xml-runtime",
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "@xml-runtime/*": ["xml-runtime/*"]
    },
    "strict": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src"
  ]
}