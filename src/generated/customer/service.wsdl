<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions
    xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
    xmlns:tns="http://example.com/customer-service"
    xmlns:cus="http://example.com/schema/customer"
    xmlns:com="http://example.com/schema/common"
    targetNamespace="http://example.com/customer-service">

  <wsdl:types>
    <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
               targetNamespace="http://example.com/customer-service"
               elementFormDefault="qualified">

      <xs:import namespace="http://example.com/schema/customer" schemaLocation="customer.xsd"/>
      <xs:import namespace="http://example.com/schema/common" schemaLocation="common.xsd"/>

      <xs:element name="GetCustomerRequest">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="CustomerId" type="xs:string"/>
            <xs:element name="Auth" type="com:AuthHeader"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>

      <xs:element name="GetCustomerResponse">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Customer" type="cus:Customer"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>

    </xs:schema>
  </wsdl:types>

</wsdl:definitions>
